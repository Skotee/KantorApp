    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-joi-validation": "^0.3.0",
    "http": "0.0.0",
    "http-status": "^1.3.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.0",
    "knex": "^0.16.3",
    "morgan": "^1.9.1",
