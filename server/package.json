{
  "name": "meme-lib",
  "version": "1.0.0",
  "description": "The first real meme app & API",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js --exec babel-node",
    "server": "nodemon server.js --exec babel-node",
    "copy-env": "babel-node bin/copyEnv.js /../.env /../.env.example",
    "lint": "prettier-eslint --write \"src/**/*.js\" \"server.js\" \"knex-cli/*.js\" \"knex-cli/**/*.js\" ",
    "migrate:latest": "knex --knexfile=knex-cli/knexfile.js migrate:latest",
    "migrate:rollback": "knex --knexfile=knex-cli/knexfile.js migrate:rollback",
    "hello": "node_modules/.bin/knex --knexfile=knex-cli/knexfile.js migrate:make meme"
  },
  "keywords": [],
